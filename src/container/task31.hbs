{{#with navigation}}
<nav>
{{#each links}}
<a class="btn btn-link" href="{{href}}">{{text}}</a>
        {{/each}}
<a class="btn btn-primary" href="{{button.href}}">{{button.text}}</a>
<hr>
</nav>
{{/with}}
<br><br>
{{#with header}}
<h1>{{title}}</h1>
<p>{{description}}</p>
<a class="btn btn-primary" href="{{button.link}}">{{button.text}}</a>

{{/with}}
<br><br>
<hr>
<br><br>
{{#with city}}
<h2>{{title}}</h2>
      
     <div class="row gx-3 gy-1"> 
       {{#each cards}}
    <div class="col-6">
     <div class="card">
       <div class="card-img-top"><img src="{{picture.src}}" alt="{{picture.alt}}">
        <div class="card-body">
    <h6 class="card-title">{{title}}</h6>
    <p>{{description}}</p>
   <a class="btn btn-primary" href="{{button.href}}">{{button.text}}</a>
   </div>
   </div>
</div>
    </div>
{{/each}}
{{/with}}
<br><br>
<hr>
<br><br>
{{#with winner}}
<h2>{{title}}</h2>
{{#with table}}
<table class="table">
 <thead>
  <tr>
    {{#each head}}
      <th>{{this}}</th>
     {{/each}}
     </tr>
 </thead>
 <tbody>

{{#each user}}
 <tr>
      <td>{{place}}</td>
      <td>{{name}}</td>
      <td>{{surname}}</td>
      <td>{{nickname}}</td>
   </tr>
   {{/each}}
 </tbody>
 </table>
{{/with}}
<br><br>
<hr>
<br><br>

{{#with gallery}}

<h2>{{title}}</h2>
<div class="row"> 
       {{#each cards}}
    <div class="col-4 gy-5">
<div class="card">
    <div class="card-header">{{name}}</div>
    {{#each items}}
<ul class="list-group list-group-flush">
      <li class="list-group-item">{{this}}</li>   
</ul>
{{/each}}
</div>
</div>
{{/each}}
</div>
{{/with}}
<br><br>
<hr>
<br><br>



<div>
{{#with relaxion}}
<div class="display-3">{{title}}</div>
   
     <div class="row gx-2 gy-4"> 
       {{#each cards}}
    <div class="col-5">
     <div class="card text-bg-secondary">
       <div class="card-header">{{header}}</div>
        <div class="card-body">
    <h6 class="card-title">{{title}}</h6>
    <p>{{description}}</p>
      </div>
   </div>
</div>
{{/each}}
</div>
{{/with}}
</div>
   <br><br>
<hr>
<br><br>

         <div>
{{#with rest}}
       <div class="row gx-2 gy-2"> 
       {{#each cards}}
    <div class="col-6">
     <div class="card  text-center">
       <div class="card-header">{{header}}</div>
        <div class="card-body">
    <h6 class="card-title">{{title}}</h6>
    <p>{{description}}</p>
      <p><button class="btn btn-primary">{{button}}</button></p>
      <div class="card-footer text-center">{{footer}}</div>
      </div>
   </div>
</div>
{{/each}}
</div>
{{/with}}
</div>
   <br>
   <br>
<hr>
<br><br>
 

      <div>
{{#with proposition}}
<div class="display-2">{{title}}</div>
       <div class="row gx-2"> 
       {{#each card}}
    <div class="col-5">
     <div class="card text-bg-warning">
       <div class="card-header text-light">{{header}}</div>
        <div class="card-body text-light">
    <h6 class="card-title text-light">{{title}}</h6>
    <p>{{description}}</p>
     
      </div>
   </div>
</div>
{{/each}}
</div>
{{/with}}
</div>
   <br>
   <br>
<hr>
<br><br>



  {{#with footer}}
      <h5> {{title}}</h5>
<div class="row ">
  <div class="col-3">
      {{#each navigation}}
<a class="text-info" href="{{href}}">{{text}}</a>
      {{/each}} 
</div>           
<div class="col-3">
   {{#each socialMedia}}  
    <button class="btn btn-outline-dark" href="{{link}}">{{name}}</button>
    {{/each}}
 </div>
 <div class="col-3">
         {{#each partners}}
      <button class="btn btn-outline-info" href="{{link}}">{{name}}</button>
       {{/each}} 
       </div> 
      </div>
